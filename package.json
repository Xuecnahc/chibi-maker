{
  "name": "maker",
  "version": "1.0.0",
  "description": "test project",
  "main": "src/server/main.js",
  "scripts": {
    "clean": "rm -r public/",
    "build": "mkdir -p public/ && cp src/client/index.html public/ && cp -r src/client/assets/ public/ && npm run relay && webpack --config webpack.config.js",
    "serve": "npm run build &> /dev/null && node src/server/main.js --port=3000",
    "start": "npm run build && node src/server/main.js --port=3000",
    "test": "mocha --require test/setup.js --compilers babel-core/register --recursive test/",
    "watch": "webpack --watch & npm run start & watchman src/client \"npm run relay\"",
    "relay": "relay-compiler --src ./src/client --schema ./src/common/schema.graphql",
    "check-update": "npm-check-updates"
  },
  "author": "pierre.hieronymus <hieronymus.pierre@gmail.com>",
